    countToBackgroundColor: (count) ->
        count = parseInt(count)
        switch
            when count < 0
                'red'
            when count == 0
                ''
            when count > 0
                i = parseInt(200 * Math.pow(0.9, count - 1))
                "rgb(#{i}, 255, #{i})"
            else
                ''

