                        when 'Scum and Villainy'
                            'scum'
                        when 'Resistance'
                            'resistance'
                        when 'First Order'
                            'firstorder'
                        when 'Galactic Republic'
                            'galacticrepublic'
                        when 'Separatist Alliance'
